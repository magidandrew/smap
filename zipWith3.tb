(* Simple testbench for your zipWith3 code.

Run this, e.g., with

ocaml < zipWith3.tb

*)

#use "zipWith3.ml";;

(* Should produce [] *)
zipWith3 (fun x z y -> x + y + z) [] [] [];;

(* Should produce [12; 15; 18] *)
zipWith3 (fun x z y -> x + y + z) [1;2;3] [4;5;6] [7;8;9];;

(* Should produce [(7, 'a'); (5, 'b')] *)
zipWith3 (fun x y z -> (x + z, y)) [5;2] ['a';'b';'c'] [2;3];;
